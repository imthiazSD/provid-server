{
  "name": "video-editing-backend",
  "version": "1.0.0",
  "description": "Express.js backend for video editing application",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "migrate": "node dist/migrations/index.js",
    "seed": "node dist/seeds/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "joi": "^17.11.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^7.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "ioredis": "^5.3.2",
    "socket.io": "^4.7.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.0",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/cors": "^2.8.15",
    "@types/multer": "^1.4.9",
    "@types/compression": "^1.7.4",
    "@types/uuid": "^9.0.6",
    "@types/nodemailer": "^6.4.11",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^8.15.1",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  }
}